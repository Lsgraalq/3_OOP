import math 
class Triangle:
    """trygolnick"""
    def checker(self,a,b,c):
        if a > b + c or b > c + a or c > b + a:
            print('Треугольник со сторонами ', a , b , c, " не может существовать")
        else:
            print('Треугольник со сторонами ', a , b , c, " может существовать")
def main():
    t = Triangle()
    a = abs(int(input("Введите 1 сторону треугольника (Число должно быть больше 0)")))
    b = abs(int(input("Введите 2 сторону треугольника (Число должно быть больше 0)")))
    c = abs(int(input("Введите 3 сторону треугольника (Число должно быть больше 0)")))
    t.checker(a,b,c)
main()
